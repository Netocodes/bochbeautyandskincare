{
  "rewrites": [
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*\\.js)", "destination": "/$1" },
    { "source": "/(.*\\.css)", "destination": "/$1" },
    { "source": "/(.*\\.png|.*\\.jpg|.*\\.svg)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}