{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
                "rewrites": [
                  {
                    "source": "/assets/:path*",
                    "destination": "/assets/:path*"
                  },
                  
                  {
                    "source": "/(.*)",
                    "destination": "/index.html"
                  }
                ],
                  "redirects": [
                    {
                      "source": "/(.*)",
                      "destination": "https://www.bochbeautyandskincare.shop/$1",
                      "permanent": true,
                      "has": [
                        {
                          "type": "host",
                          "value": "bochbeautyandskincare.shop"
                        }
                      ]
                    }
                  ]
                      
              }