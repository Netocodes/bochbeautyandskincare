{
                "rewrites": [
                  {
                    "source": "/assets/:path*",
                    "destination": "/assets/:path*"
                  },
                  
                  {
                    "source": "/(.*)",
                    "destination": "/index.html"
                  }
                ],
                     "redirects": [
                       {
                         "source": "/:path*",
                         "destination": "https://www.bochbeautyandskincare.shop/:path*",
                         "permanent": true
                       }
                     ]            
              }