{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ],
  "build": {
    "env": {
      "VITE_BASE_PATH": "/bochbeautyandskincare"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]

}